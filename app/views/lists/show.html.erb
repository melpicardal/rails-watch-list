<div class="container">
  <h1>This shows <%= @list.name %></h1>
    <% @list.bookmarks.each do |bookmark| %>
      <p><%= bookmark.movie.title %></p>
      <p><%= link_to "delete", bookmark_path(bookmark), data: { confirm: 'you sure?' },  method: :delete%>
    <% end %>
  <p><%= link_to "add a movie", new_list_bookmark_path(@list) %> </p>
</div>
